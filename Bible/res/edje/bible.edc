               rel2.relative: 0.0 1.0;
               align: 0.0 0.0;
               visible: 0;
            }
            description { state: "bookmark" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
            description { state: "note" 0.0;
               inherit: "bookmark" 0.0;
               color: 0 0 255 255;
            }
            description { state: "bookmarknote" 0.0;
               inherit: "bookmark" 0.0;
               rel2.relative: 0.0 0.5;
