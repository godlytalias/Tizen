         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "bookmark_style_night_mode";
         base: "font=Tizen:style=Bold align=left text_class=GTA1B font_size=25 color=#ffffff wrap=mixed";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "note_style";
         base: "font=Tizen:style=Light align=left text_class=GTA1L font_size=25 color=#000000 wrap=mixed ellipsis=1.0";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "verse_style";
         base: "font=Tizen:style=Regular align=left text_class=GTA1 font_size=25 color=#000000 wrap=mixed ellipsis=1.0";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "verse_count_style";
         base: "font=Tizen:style=Light align=center font_size=15 color=#000000 wrap=mixed";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "verse_count_style_night_mode";
         base: "font=Tizen:style=Regular align=center font_size=15 color=#ffffff wrap=mixed";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
      style {
         name: "verse_reference_style";
         base: "font=Tizen:style=Light align=left font_size=20 color=#000000 wrap=mixed";
         tag: "br" "\n";
         tag: "ps" "ps";
         tag: "hilight" "+ font_weight=Bold";
         tag: "b" "+ font_weight=Bold";
         tag: "tab" "\t";
      }
   }
   
   plugins {
      plugin {
         name: "touch_sound";
         source: "feedback";
         param: "FEEDBACK_TYPE_SOUND FEEDBACK_PATTERN_TAP";
      }
   }

   group { name: "main";
      images.image: "Bible.png" COMP;
      images.image: "left_arrow.png" COMP;
      images.image: "left_arrow_white.png" COMP;
      images.image: "right_arrow.png" COMP;
      images.image: "right_arrow_white.png" COMP;
      script {
         public click_mod = 0;
         public night_mode = 0;
      }
      parts {
         part {name: "base";
            scale: 1;
            type: RECT;
            description { state: "default" 0.0;
               rel1.relative: 0.0 0.0;
               rel2.relative: 1.0 1.0;
               color: 0 0 0 255;
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               color: 0 0 0 0;
            }
         }
         part { name: "app_icon";
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "base";
               rel2.to: "base";
               min: 100 100;
               max: 100 100;
               image.normal: "Bible.png";
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.text.copyright";
            scale: 1;
            type: TEXTBLOCK;
            description { state: "default" 0.0;
               min: 480 40;
               fixed: 1 1;
               rel1.to: "app_icon";
               rel2.to: "app_icon";
               rel2.relative: 1.0 0.0;
               align: 0.5 1.0;
               text.min: 1 1;
               text.style: "subtitle_style";
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.text.version";
            scale: 1;
            type: TEXTBLOCK;
            description { state: "default" 0.0;
               min: 480 40;
               fixed: 1 1;
               rel1.to: "elm.text.copyright";
               rel2.to: "elm.text.copyright";
               rel2.relative: 1.0 0.0;
               align: 0.5 1.0;
               text.min: 1 1;
               text.style: "subtitle_style";
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.text.apptitle";
            scale: 1;
            type: TEXTBLOCK;
            description { state: "default" 0.0;
               min: 480 80;
               fixed: 1 1;
               rel1.to: "elm.text.version";
               rel2.to: "elm.text.version";
               rel2.relative: 1.0 0.0;
               align: 0.5 1.0;
               text.min: 1 1;
               text.style: "title_style";
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.swallow.progressbar";
            scale: 1;
            type: SWALLOW;
            description { state: "default" 0.0;
               min: 480 80;
               fixed: 1 1;
               rel1.to_y: "app_icon";
               rel1.to_x: "base";
               rel1.relative: 0.0 1.0;
               rel2.to_y: "app_icon";
               rel2.to_x: "base";
               align: 0.5 0.0;
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.text.loading";
            scale: 1;
            type: TEXTBLOCK;
            description { state: "default" 0.0;
               min: 480 40;
               fixed: 1 1;
               rel1.to: "elm.swallow.progressbar";
               rel1.relative: 0.0 1.0;
               rel2.to: "elm.swallow.progressbar";
               text.min: 1 1;
               align: 0.5 0.0;
               text.style: "subtitle_style";
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "clipper";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "base";
               rel2.to: "base";
               color: 0 0 0 0;
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               color: 255 255 255 255;
            }
         }
         part { name: "elm.header.part";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
              min: 480 104;
              fixed: 1 0;
              rel1.to: "base";
              rel2.to: "base";
              color: 0 0 0 230;
              align: 0.5 0.0;
              rel2.relative: 1.0 0.0;
            }
         }
         part { name: "elm.header.left_top.padding";
            type: SPACER;
            scale: 1;
            description { state: "default" 0.0;
              min: 32 32;
              fixed: 1 1;
              rel1.to: "elm.header.part";
              rel2.to: "elm.header.part";
              rel2.relative: 0.0 0.0;
              align: 0.0 0.0;
            }
         }
         part { name: "elm.header.right_bottom.padding";
            type: SPACER;
            scale: 1;
            description { state: "default" 0.0;
              min: 32 32;
              fixed: 1 1;
              rel1.to: "elm.header.part";
              rel1.relative: 1.0 1.0;
              rel2.to: "elm.header.part";
              rel2.relative: 1.0 1.0;
              align: 1.0 1.0;
            }
         }
         part { name: "elm.text.book_title";
            type: TEXTBLOCK;
            scale: 1;
            clip_to: "clipper";
            description { state: "default" 0.0;
               text.style: "title_style";
               text.min: 1 1;
               rel1.to: "elm.header.left_top.padding";
               rel1.relative: 1.0 0.0;
               rel2.to: "elm.header.right_bottom.padding";
               rel2.relative: 0.0 1.0;
            }
         }
         part { name: "title_left_button";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "base";
               rel2.to: "elm.header.part";
               rel2.relative: 0.0 1.0;
               align: 0.0 0.5;
               min: 80 40;
               visible: 0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part { name: "title_right_button";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "base";
               rel1.relative: 1.0 0.0;
               rel2.to: "elm.header.part";
               align: 1.0 0.5;
               min: 80 40;
               visible: 0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part { name: "change_book_event";
            type: RECT;
            scale: 1;
            mouse_events: 1;
            repeat_events: 0;
            ignore_flags: ON_HOLD;
            description { state: "default" 0.0;
               color: 0 0 0 0;
               rel1.to: "elm.header.part";
               rel2.to: "elm.header.part";
               visible: 0;
            }
            description { state: "loaded" 0.0;
               inherit: "default" 0.0;
               visible: 1;
            }
         }
         part {name: "select_all_clipper";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "elm.header.part";
               rel1.relative: 0.0 1.0;
               rel2.to: "elm.select.all";
               align: 0.5 0.0;
               color: 255 255 255 255;
            }
         }
         part {name: "elm.select.all";
            type: SWALLOW;
            scale: 1;
            clip_to: "select_all_clipper";
            description { state: "default" 0.0;
               rel1.to: "elm.header.part";
               rel1.relative: 0.0 1.0;
               rel2.to: "elm.header.part";
               min: 480 80;
               fixed: 1 1;
               align: 0.5 1.0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               align: 0.5 0.0;
            }
         }
         part {name: "elm.swallow.content";
            type: SWALLOW;
            scale: 1;
            clip_to: "clipper";
            description { state: "default" 0.0;
               rel1.to_y: "elm.select.all";
               rel1.to_x: "base";
               rel1.relative: 0.0 1.0;
               rel2.to: "base";
            }
        }
        part { name: "prev_btn";
           scale: 1;
           clip_to: "clipper";
           description { state: "default" 0.0;
              rel1.to: "base";
              rel1.relative: 0.0 1.0;
              rel2.to: "base";
              rel2.relative: 0.0 1.0;
              min: 32 32;
              fixed: 1 1;
              align: 0.0 1.0;
              color: 255 255 255 50;
              image.normal: "left_arrow.png";
              visible: 0;
           }
          description { state: "loaded" 0.0;
             inherit: "default" 0.0;
             visible: 1;
          }
          description { state: "highlight" 0.0;
             inherit: "loaded" 0.0;
             color: 255 255 255 100;
          }
          description { state: "clicked" 0.0;
             inherit: "loaded" 0.0;
             color: 255 255 255 200;
          }
          description { state: "loaded_night" 0.0;
             inherit: "loaded" 0.0;
             image.normal: "left_arrow_white.png";
          }
          description { state: "highlight_night" 0.0;
             inherit: "loaded_night" 0.0;
             color: 255 255 255 150;
          }
          description { state: "clicked_night" 0.0;
             inherit: "loaded_night" 0.0;
             color: 255 255 255 250;
          }
        }
        part { name: "nxt_btn";
           scale: 1;
           clip_to: "clipper";
           description { state: "default" 0.0;
              rel1.to: "base";
              rel1.relative: 1.0 1.0;
              rel2.to: "base";
              rel2.relative: 1.0 1.0;
              min: 32 32;
              fixed: 1 1;
              align: 1.0 1.0;
              image.normal: "right_arrow.png";
              visible: 0;
          }
          description { state: "loaded" 0.0;
             inherit: "default" 0.0;
             color: 255 255 255 50;
             visible: 1;
          }
          description { state: "highlight" 0.0;
             inherit: "loaded" 0.0;
             color: 255 255 255 100;
          }
          description { state: "clicked" 0.0;
             inherit: "loaded" 0.0;
             color: 255 255 255 200;
          }
          description { state: "loaded_night" 0.0;
             inherit: "loaded" 0.0;
			 image.normal: "right_arrow_white.png";
          }
          description { state: "highlight_night" 0.0;
             inherit: "loaded_night" 0.0;
             color: 255 255 255 150;
          }
          description { state: "clicked_night" 0.0;
             inherit: "loaded_night" 0.0;
             color: 255 255 255 250;
          }
        }
     }
     programs {
        program { name: "signal_emit";
           action: SIGNAL_EMIT "elm,holy_bible,book,change" "elm";
        }
        program { name: "select_all_show";
           action: STATE_SET "visible" 0.0;
           target: "elm.select.all";
           transition: LINEAR 0.2;
        }
        program { name: "select_all_hide";
           action: STATE_SET "default" 0.0;
           target: "elm.select.all";
           transition: LINEAR 0.2;
        }
        program { name: "night_mode_on";
           signal: "elm,holy_bible,night_mode,on";
           source: "elm";
           script {
              set_state(PART:"nxt_btn", "loaded_night", 0.0);
              set_state(PART:"prev_btn", "loaded_night", 0.0);
              set_int(night_mode, 1);
           }
        }
        program { name: "night_mode_off";
           signal: "elm,holy_bible,night_mode,off";
           source: "elm";
           script {
              set_state(PART:"nxt_btn", "loaded", 0.0);
              set_state(PART:"prev_btn", "loaded", 0.0);
              set_int(night_mode, 0);
           }
        }
        program { name: "share_copy_mode_on";
           signal: "elm,holy_bible,share_copy,on";
           source: "elm";
           script {
              set_state(PART:"title_left_button", "visible", 0.0);
              set_state(PART:"title_right_button", "visible", 0.0);
              set_state(PART:"change_book_event", "default", 0.0);
              set_state(PART:"nxt_btn", "default", 0.0);
              set_state(PART:"prev_btn", "default", 0.0);
           }
           after: "select_all_show";
        }
        program { name: "share_copy_mode_off";
           signal: "elm,holy_bible,share_copy,off";
           source: "elm";
           script {
              set_state(PART:"title_left_button", "default", 0.0);
              set_state(PART:"title_right_button", "default", 0.0);
              set_state(PART:"change_book_event", "loaded", 0.0);
              if (get_int(night_mode) == 0)
              {
                 set_state(PART:"nxt_btn", "loaded", 0.0);
                 set_state(PART:"prev_btn", "loaded", 0.0);
              }
              else
              {
                 set_state(PART:"nxt_btn", "loaded_night", 0.0);
                 set_state(PART:"prev_btn", "loaded_night", 0.0);
              }
           }
           after: "select_all_hide";
        }
        program { name: "change_book_event_run";
           action: RUN_PLUGIN "touch_sound";
           after: "signal_emit";
        }
        program { name: "change_book_down";
           signal: "mouse,down,*";
           source: "change_book_event";
           script {
              set_int(click_mod, 1);
           }
        }
        program { name: "change_book_up";
           signal: "mouse,up,*";
           source: "change_book_event";
           script {
              if (get_int(click_mod) < 10) {
                 run_program(PROGRAM:"change_book_event_run");
              }
           }
        }
        program { name: "change_book_mouse_move";
           signal: "mouse,move";
           source: "change_book_event";
           script {
              set_int(click_mod, get_int(click_mod)+1);
           }
        }
        program { name: "next_chap_signal";
           action: SIGNAL_EMIT "elm,holy_bible,next,chapter" "elm";
        }
        program { name: "next_chap";
           signal: "mouse,clicked,*";
           source: "nxt_btn";
           action: RUN_PLUGIN "touch_sound";
           after: "next_chap_signal";
        }
        program { name: "prev_chap_signal";
           action: SIGNAL_EMIT "elm,holy_bible,prev,chapter" "elm";
        }
        program { name: "prev_chap";
           signal: "mouse,clicked,*";
           source: "prev_btn";
           action: RUN_PLUGIN "touch_sound";
           after: "prev_chap_signal";
        }
        program { name: "arrow_unhighlight_day";
           transition: LINEAR 0.3;
           action: STATE_SET "loaded" 0.0;
           target: "nxt_btn";
           target: "prev_btn";
        }
        program { name: "arrow_unhighlight_night";
           transition: LINEAR 0.3;
           action: STATE_SET "loaded_night" 0.0;
           target: "nxt_btn";
           target: "prev_btn";
        }
        program { name: "prev_btn_pressed";
           signal: "mouse,down,*";
           source: "prev_btn";
		   script {
		      if (get_int(night_mode) == 0)
		         set_state(PART:"prev_btn", "clicked", 0.0);
		      else
		         set_state(PART:"prev_btn", "clicked_night", 0.0);
		   }
        }
        program { name: "prev_btn_released";
           signal: "mouse,up,*";
           source: "prev_btn";
		   script {
		      if (get_int(night_mode) == 0)
		         run_program(PROGRAM:"arrow_unhighlight_day");
		      else
		         run_program(PROGRAM:"arrow_unhighlight_night");
		   }
        }
        program { name: "nxt_btn_pressed";
           signal: "mouse,down,*";
           source: "nxt_btn";
		   script {
		      if (get_int(night_mode) == 0)
		         set_state(PART:"nxt_btn", "clicked", 0.0);
		      else
		         set_state(PART:"nxt_btn", "clicked_night", 0.0);
		   }
        }
        program { name: "nxt_btn_released";
           signal: "mouse,up,*";
           source: "nxt_btn";
		   script {
		      if (get_int(night_mode) == 0)
		         run_program(PROGRAM:"arrow_unhighlight_day");
		      else
		         run_program(PROGRAM:"arrow_unhighlight_night");
		   }
        }
        program { name: "arrow_highlight_day";
           transition: LINEAR 0.2;
           action: STATE_SET "highlight" 0.0;
           target: "nxt_btn";
           target: "prev_btn";
           after: "arrow_unhighlight_day";
        }
        program { name: "arrow_highlight_night";
           transition: LINEAR 0.2;
           action: STATE_SET "highlight_night" 0.0;
           target: "nxt_btn";
           target: "prev_btn";
           after: "arrow_unhighlight_night";
        }
        program { name: "arrow_highlight";
           signal: "elm,holy_bible,arrow,highlight";
           source: "elm";
		   script {
		      if (get_int(night_mode) == 0)
		         run_program(PROGRAM:"arrow_highlight_day");
		      else
				 run_program(PROGRAM:"arrow_highlight_night");
		   }
        }
        program { name: "splash_finish";
           action: SIGNAL_EMIT "elm,holy_bible,splash,over" "elm";
        }
        program { name: "loading_done";
           signal: "elm,holy_bible,loading,done";
           source: "elm";
           in: 1.0 0.0;
           transition: LINEAR 0.3;
           action: STATE_SET "loaded" 0.0;
           target: "base";
           target: "clipper";
           target: "change_book_event";
           target: "app_icon";
           target: "elm.text.apptitle";
           target: "elm.text.version";
           target: "elm.text.copyright";
           target: "elm.text.loading";
           target: "elm.swallow.progressbar";
           target: "nxt_btn";
           target: "prev_btn";
           after: "splash_finish";
        }
     }
   }
   
   group{ name: "select_all_layout";
      parts {
         part { name: "base";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               color: 255 255 255 255;
               min: 480 80;
            }
         }
         part { name: "elm.padding.left_top";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               color: 0 0 0 0;
               min: 32 10;
               fixed: 1 1;
               rel1.to: "base";
               rel2.to: "base";
               rel2.relative: 0.0 1.0;
            }
         }
         part { name: "elm.padding.right_bottom";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               color: 0 0 0 0;
               min: 32 10;
               fixed: 1 1;
               rel1.to: "base";
               rel1.relative: 1.0 0.0;
               rel2.to: "base";
            }
         }
         part { name: "select_all";
            type: TEXTBLOCK;
            scale: 1;
            description { state: "default" 0.0;
               text.style: "main_verse_style";
               min: 320 60;
               fixed: 1 1;
               rel1.to: "elm.padding.left_top";
               rel1.relative: 1.0 1.0;
               rel2.to_x: "elm.padding.left_top";
               rel2.to_y: "elm.padding.right_bottom";
               rel2.relative: 1.0 0.0;
