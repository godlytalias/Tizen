               rel2.to: "base";
               rel2.relative: 0.0 0.0;
               align: 0.0 0.0;
            }
         }
         part{ name: "right_bottom_padding";
            type: SPACER;
            scale: 1;
            description { state: "default" 0.0;
               min: 32 24;
               fixed: 1 1;
               rel1.to: "base";
               rel1.relative: 1.0 1.0;
               rel2.to: "base";
               align: 1.0 1.0;
            }
         }
         part {name: "elm.text.reference";
            type: TEXTBLOCK;
            scale: 1;
            description {state: "default" 0.0;
               rel1.to: "left_top_padding";
               rel1.relative: 1.0 1.0;
               rel2.to_x: "right_bottom_padding";
               rel2.to_y: "left_top_padding";
               rel2.relative: 0.0 1.0;
               text.style: "verse_reference_style";
               fixed: 1 1;
               text.min: 0 0;
               min: 416 40;
               align: 0.0 0.0;
            }
        }
         part{ name: "elm.text.verse";
            type: TEXTBLOCK;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to_y: "elm.text.reference";
               rel1.to_x: "left_top_padding";
               rel1.relative: 1.0 1.0;
               rel2.to: "right_bottom_padding";
               rel2.relative: 0.0 0.0;
               text.style: "verse_style";
               fixed: 1 1;
               text.min: 0 0;
               min: 416 80;
               align: 0.0 1.0;
            }         
         }
     }
   }
   
  group { name: "bookmarks_layout";
      alias: "notes_layout";
      parts {
         part { name: "content_bg";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               color: 230 230 230 230;
               visible: 1;
            }
            description { state: "hide" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "no_content_text";
            type: TEXTBLOCK;
            scale:1;
            description { state: "default" 0.0;
               rel1.to: "content_bg";
               rel2.to: "content_bg";
               text.style: "verse_count_style";
               text.text: "No items to display!";
            }
            description { state: "hide" 0.0;
               inherit: "default" 0.0;
               visible: 0;
            }
         }
         part { name: "elm.swallow.content";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
               rel1.to: "content_bg";
               rel2.to: "content_bg";
               visible: 0;
            }
            description { state: "hide" 0.0; //actually show
               inherit: "default" 0.0;
               visible: 1;
            }
         }
      }
      programs {
         program { name: "content_bg_hide";
            signal: "elm,holy_bible,bg,hide";
            source: "elm";
            action: STATE_SET "hide" 0.0;
            target: "content_bg";
            target: "elm.swallow.content";
            target: "no_content_text";
         }
         program { name: "content_bg_show";
            signal: "elm,holy_bible,bg,show";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "content_bg";
            target: "elm.swallow.content";
            target: "no_content_text";
         }
      }
   }
   
