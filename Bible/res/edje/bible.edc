           target: "selection_marker";
        }
        program { name: "selection_hide";
           signal: "elm,holy_bible,selection,hide";
           source: "elm";
           action: STATE_SET "default" 0.0;
           transition: LINEAR 0.3;
           target: "selection_marker";
        }
        program { name: "bookmark_nightmode_activate";
        	script {
        		if (get_int(bookmark) == 1)
        		{
        			set_state(PART:"elm.text.verse", "bookmark_night_mode", 0.0);
        		}
        	}
        }
        program { name: "bookmark_nightmode_deactivate";
        	script {
        		if (get_int(bookmark) == 1)
        		{
        			set_state(PART:"elm.text.verse", "bookmark", 0.0);
        		}
        	}
        }
        program { name: "night_mode_activate";
           signal: "elm,holy_bible,night_mode,on";
           source: "elm";
           action: STATE_SET "night_mode" 0.0;
           target: "base";
           target: "elm.text.verse";
           target: "elm.text.verse_count";
           after: "bookmark_nightmode_activate";
        }
        program { name: "night_mode_deactivate";
           signal: "elm,holy_bible,night_mode,off";
           source: "elm";
           action: STATE_SET "default" 0.0;
           target: "base";
           target: "elm.text.verse";
           target: "elm.text.verse_count";
           after: "bookmark_nightmode_deactivate";
        }
        program { name: "bookmark_activate_script";
           script
           {
              new st[31], Float:vl;
              get_state(PART:"elm.text.verse_count", st, 30, vl);
              if (!strcmp(st, "night_mode")) {
                 set_state(PART:"elm.text.verse", "bookmark_night_mode", 0.0);
              }
              set_int(bookmark, 1);
           }
        }
        program { name: "bookmark_show";
           signal: "elm,holy_bible,bookmark,show";
           source: "elm";
           action: STATE_SET "bookmark" 0.0;
           target: "bookmark_note_label";
           target: "elm.text.verse";
           after: "bookmark_activate_script";
        }
        program { name: "note_show";
           signal: "elm,holy_bible,note,show";
           source: "elm";          
           script {
              new st[31], Float:vl;
              get_state(PART:"bookmark_note_label", st, 30, vl);
              if (!strcmp(st, "bookmark")) {
                 set_state(PART:"bookmark_note_label", "bookmarknote", 0.0);
                 set_state(PART:"note_label", "bookmarknote", 0.0);
              }
              else {
                 set_state(PART:"bookmark_note_label", "note", 0.0);
              }
           }
        }
     }
   }
   
