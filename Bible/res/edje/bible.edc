  group { name: "bookmarks_layout";
      parts {
         part { name: "elm.swallow.content";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
            }
         }
      }
   }

   group { name: "select_chapter_layout";
      parts {
         part { name: "elm.swallow.content1";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
               fixed: 1 0;
               rel1.relative: 0.0 0.0;
               rel2.relative: 0.70 1.0;
            }
         }
         part { name: "divider";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
               fixed: 1 0;
               color_class: "B011";
               rel1.to: "elm.swallow.content1";
               rel1.relative: 1.0 0.0;
               rel2.to: "elm.swallow.content1";
               min: 2 0;
            }
         }
         part { name: "elm.swallow.content2";
